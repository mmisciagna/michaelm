  "scripts": {
    "build": "rm -rf dist && npm run build:prod",
    "build:prod": "webpack --mode=production --devtool=false",
    "clean": "rm -rf dist node_modules package-lock.json",
    "dev": "run-p webpack:start",
    "stage:version": "npm run build && gcloud app deploy . --project michaelm --appyaml=app.yaml --no-promote --quiet --version",
    "stage:promote": "npm run build && gcloud app deploy . --project michaelm --appyaml=app.yaml --promote --quiet --version master",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack:start": "webpack serve"
  },
